<project xmlns="http://maven.apache.org/DECORATION/1.4.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.4.0 http://maven.apache.org/xsd/decoration-1.4.0.xsd"
         name="${project.name}"
>
    <!-- Fluido  Skin -->
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.4</version>
    </skin>
    <custom>
        <fluidoSkin>
            <topBarEnabled>true</topBarEnabled>
            <sideBarEnabled>true</sideBarEnabled>
        </fluidoSkin>
    </custom>
    <body>
        <!--
        <menu name="${project.name}">
            <item name="Overview" href="index.html" />
        </menu>
        -->
        <breadcrumbs>
            <item name="Documentation" href="../index.html" />
        </breadcrumbs>
        <links>
            <item name="GitHub" href="http://github.com/tawja/" />
        </links>
        <menu name="Documentation" inherit="top">
            <item name="Overview" href="index.html" />
        </menu>
        <menu ref="reports" inherit="top" />
        <menu ref="parent" inherit="top" />
        <menu ref="modules" inherit="top" />
        <menu name="Child Projects" inherit="top">
            <item name="Tawja Project" href="../index.html" />
        </menu>
        <menu ref="parent" inherit="bottom"/>
        <menu ref="modules" inherit="bottom"/>
        <menu ref="reports" inherit="bottom"/>
    </body>
</project>